@model IEnumerable<MNSuperadmin.Models.UserInfo>
@{
    ViewBag.Title = "RegistrationRejectedList";
}

<section class="content-header">
    <h1>
        Agent
        <small></small>
    </h1>
    <ol class="breadcrumb">
        <li><a href="#"><i class="fa fa-user"></i>Agent @*@ViewContext.RouteData.Values["Controller"].ToString()*@</a></li>
        <li class="active">Registration Rejected List</li>
    </ol>
</section>

<!-- Main content -->
<section class="content">
    <!-- Main row -->
    <div class="row">
        <div class="col-md-12">
            <!-- Start Form -->
            <!-- Start Data Show in Table -->
            <div id="AdminApprove" class="box box-primary">
                <div class="box-header with-border">
                    <h3 class="box-title">Registration Rejected List</h3>
                </div>
                <!-- /.box-header -->




                @if (this.ViewData["agenttmodify_messsage"] != null)
                {
                    if (this.ViewData["message_class"].ToString() == "success_info")
                    {
                        <div class="alert alert-success">
                            <button type="button" class="close" data-dismiss="alert">&times;</button>
                            <h4>Success</h4> @this.ViewData["agenttmodify_messsage"]
                        </div>
                    }
                    else
                    {
                        <div class="alert alert-danger">
                            <button type="button" class="close" data-dismiss="alert">&times;</button>
                            <h4>Error</h4> @this.ViewData["agentmodify_messsage"]
                        </div>
                    }
                }

                <div class="box-body">
                    <div class="box box-primary">

                        @using (Html.BeginForm("RegistrationRejectedList", "Agent", FormMethod.Get))
                        {
                            <div class="box-body">
                                <div class="row">
                                    <!-- radio -->
                                    <div class="form-group">
                                        <div class="radio col-md-12">
                                            <div class="col-md-12">
                                                <label>
                                                    <input type="radio" name="Key" id="optionsRadios1" value="UserName" checked />
                                                    <b>User Name</b>
                                                </label>

                                            </div>
                                        </div>
                                    </div>


                                    <div>
                                        <hr />
                                    </div>

                                    <div class="form-group" id="form">
                                        <div class="col-md-6 form-group">
                                            <input type="text" class="form-control" id="txtUserName" name="Value" placeholder="Search Value" value="@ViewBag.Value" maxlength="15">
                                        </div>
                                    </div>
                                </div>

                            </div>
                        <!-- /.box-body -->
                            <div class="box-footer">
                                <input type="submit" class="btn btn-primary" id="btnSearch" value="Search" />
                            </div>
                        }

                        <hr />

                        <div class="box-body" id="grid">
                            <table id="PinApprove" class="table table-striped table-bordered">
                                <thead>
                                    <tr>
                                        <th style="display:none;">ClientCode</th>
                                        <th>Agent Name</th>
                                        <th>User Name</th>
                                        <th>Address</th>
                                        <th>Email Address</th>
                                        <th>Status</th>
                                        @*<th>Branch Code</th>
                                            <th>Branch Name</th>
                                            <th>User Role</th>*@
                                        <th>Created By</th>
                                        <th>Action</th>

                                    </tr>
                                </thead>
                                <tbody>
                                    @if (Model.Count() > 0)
                                    {
                                        foreach (var item in Model)
                                        {
                                            <tr>
                                                <td style="display:none;">@item.ClientCode</td>
                                                <td>@item.Name</td>
                                                <td>@item.UserName</td>
                                                <td>@item.Address</td>
                                                <td>@item.EmailAddress</td>
                                                <td>@item.Status</td>
                                                @*<td>@item.UserBranchCode</td>
                                                    <td>@item.UserBranchName</td>
                                                    <td>@item.AProfileName</td>*@
                                                <td>@item.CreatedBy</td>
                                                <td>
                                                    <a href="@Url.Action("AgentRegistrationRejected","Agent", new {id = @item.ClientCode } )"><span class="fa fa-edit" title="Edit"></span></a> @*Edit*@
                                                </td>


                                            </tr>
                                        }
                                    }
                                    else
                                    {
                                        <tr>
                                            <td colspan="7" style="text-align:center">NO RECORDS FOUND!</td>
                                        </tr>
                                    }
                                </tbody>
                            </table>
                        </div>
                    </div>





                </div>
                <!-- /.box-body -->

            </div>

        </div>
    </div>
    <!-- /.row -->
</section>
<!-- /.content -->
