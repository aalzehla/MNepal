
@model MNSuperadmin.ViewModel.RegCusDetailVM
@{
    ViewBag.Title = "AgentCommission Rejected List";

    
}
<link href="~/Content/plugins/jquery-ui/jquery-ui-1.12.1.custom/jquery-ui.css" rel="stylesheet" />
<style>
    thead input {
        width: 100%;
        padding: 3px;
        box-sizing: border-box;
        color: black;
    }
</style>

<!-- =============================================== -->
<!-- Content Header (Page header) -->
<section class="content-header">
    <h1>
        Agent Commission
        @*<small>WalletCustStatusNew</small>*@
    </h1>
    <ol class="breadcrumb">
        <li><a href="#"><i class="fa fa-user"></i> Agent Commission</a></li>
        <li class="active">Rejected List</li>
    </ol>
</section>

<!-- Main content -->
<section class="content">
    <!-- Main row -->
    <div class="row">
        @if (this.TempData["editregister_messsage"] != null)
        {
        if (this.TempData["message_class"].ToString() == "success_info")
        {
        <div class="alert alert-success">
            <button type="button" class="close" data-dismiss="alert">&times;</button>
            <h4>Success</h4> @this.TempData["editregister_messsage"]
        </div>
        }
        else
        {
        <div class="alert alert-danger">
            <button type="button" class="close" data-dismiss="alert">&times;</button>
            <h4>Error</h4> @this.TempData["editregister_messsage"]
        </div>
        }
        }
        <!-- Start -->
        <div class="col-xs-12">

            <div class="box box-primary">
                <div class="box-header with-border">
                    <h3 class="box-title">Agent Commission Rejected List</h3>
                </div>
                <div class="box-body">
                    <br />
                    <div class="row">
                        <div class="col-md-6">
                            <span style="color:red">@ViewBag.error</span>
                        </div>
                    </div>
                    <div class="row">
                        <div class="form-group" id="form">
                            <div id="BranchCode" class="col-md-6 form-group" style="display:none;">
                                <label>Branch Code</label>
                            </div>

                            <div id="Id" class="col-md-4 form-group">
                                <label>Id</label>
                                @Html.TextBoxFor(x => x.Parameter.Id, new { @class = "form-control", @id = "Id" })
                            </div>

                         

                        </div>
                    </div>
                </div>
                <!-- /.box-body -->
                <div class="box-footer">
                    <input type="submit" class="btn btn-primary" id="btnSearch" name="txtSearch" value="Search" />
                </div>
            </div>
            <div id="data" class="box box-primary">
                <div class="box-body" id="grid">
                    <span class="fa fa-2x fa-file-excel-o pull-right" style="color:green;display:none;" title="To Excel" id="ToExcel"></span>
                    <table id="SearchResultTable" class="table table-striped table-bordered" style="display:none">
                        <thead>
                            <tr>
                                <th>Id</th>
                                <th>FeeId</th>
                                <th>TieredStart</th>

                                <th>TieredEnd</th>
                                <th>MinAmt</th>
                                <th>MaxAmt</th>

                                <th>Percentage</th>
                                <th>FlatFee</th>
                                <th>FeeType</th>
                                <th>Edit</th>
                            </tr>
                        </thead>
                    </table>
                </div>
                <div class="overlay" id="loading" style="display:none;">
                    <i class="fa fa-refresh fa-spin"></i>
                </div>
            </div>
        </div>
    </div>
</section>
<!-- /.content -->
<!-- =============================================== -->

@section scripts{
    <script src="~/Content/plugins/jquery-ui/jquery-ui-1.12.1.custom/jquery-ui.js"></script>
    <script>
        function isNumber(evt) {
            evt = (evt) ? evt : window.event;
            var charCode = (evt.which) ? evt.which : evt.keyCode;
            if (charCode > 31 && (charCode < 48 || charCode > 57)) {
                return false;
            }
            return true;
        }
        $(document).ready(function () {
        //    debugger;
        //    var changed = "T";
        //        //$('#MobileNumber,#CustomerName').on('input propertychange paste', function () {
        //         $('#FeeId').on('input propertychange paste', function () {

        //        changed = "T";
        //    });
            //$('#HasKYC').on('change', function () {
            //    changed = "T";
            //});

            var table;
            $('#ToExcel').click(function () {
                var FeeId = $('#FeeId').val();
                //var CustomerName = $('#CustomerName').val();
                //var HasKYC = $('#HasKYC').val();

                var Parachanged = changed;
                $.ajax({
                    url: "@Url.Action("AgentCommisionRejectedTable", "AgentCommission")",
                    data: {
                        FeeId: FeeId,
                        //CustomerName: CustomerName,
                        //change: Parachanged,
                        //HasKYC: HasKYC,
                        ToExcel: 'T'
                    },
                    type: 'POST',
                    success: function (data) {
                        var url = "@Url.Action("Download", "AgentCommission")";
                        window.location = url + '?fileGuid=' + data.FileGuid
                           + '&filename=' + data.FileName;
                        /*window.open(url + '?fileGuid=' + data.FileGuid
                           + '&filename=' + data.FileName, '_blank');*/
                    },
                    error: function (data) {

                    }


                });
            });
            $('#btnSearch').click(function () {
                debugger;
                changed = "T";
                //var FeeId = $('#FeeId').val();
                var Id = $('#Id').val();
                //var CustomerName = $('#CustomerName').val();
                //var HasKYC = $('#HasKYC').val();
                var Parachanged = changed;
                if ($.fn.DataTable.isDataTable('#SearchResultTable')) {
                    table.destroy();
                }
                $('#loading').show();
                $('#btnSearch').prop("disabled", true);
                table = $('#SearchResultTable').DataTable({

                    searching: false,
                    "proccessing": true,
                    "serverSide": true,
                    "aaSorting": [],
                    orderCellsTop: true,
                    fixedHeader: true,
                    "ajax": {
                        data: function (d) {
                            //d.FeeId = FeeId;
                            d.Id = Id;
                            //d.CustomerName = CustomerName;
                            //d.change = Parachanged;
                            //d.HasKYC = HasKYC;
                        },
                        url: "@Url.Action("AgentCommisionRejectedTable", "AgentCommission")",
                        type: 'POST',

                    },
                    "columns": [
                        { "data": "Id" },
                        { "data": "FeeId" },
                        { "data": "TieredStart" },

                        { "data": "TieredEnd" },
                        { "data": "MinAmt" },
                        { "data": "MaxAmt" },

                        { "data": "Percentage" },
                        { "data": "FlatFee" },
                        { "data": "FeeType" },
                        
                      // { "defaultContent": "Self Registration" },

                        //test
                        {
                            "data": "Id",
                               "render": function (data)
                               {
                                   return "<a href='../AgentCommission/ViewServiceCommissionDetail/" + data + "'><i class='fa fa-edit' '></i></a>"/*style='margin-left: 91px;*/
                               }
                           },
                        //{
                        //    "data": "FeeId",
                        //       "render": function (data)
                        //       {
                        //           return "<a href='../AgentCommission/ViewSelfRegDetailWalletCustStatus/" + data + "'><i class='fa fa-eye' style='margin-left: 91px;'></i></a>"
                        //       }
                        //   },
                    ],
                    "drawCallback": function (settings) {
                        changed = "F";
                        Parachanged = changed;
                        $('#SearchResultTable').show();
                        if (settings.json.recordsTotal > 0)
                            $('#ToExcel').show();
                        else
                            $('#ToExcel').hide();
                        $('#loading').hide();
                        $('#btnSearch').prop("disabled", false);
                    },

                });

            });
            $('#grid').on("DOMSubtreeModified", function () {
                changed = "F";
            });
        });

        $(document).ready(function () {
            $.validator.addMethod("pattern", function (value, element, param) {
                if (this.optional(element)) {
                    return true;
                }
                if (typeof param === "string") {
                    param = new RegExp("^(?:" + param + ")$");
                }
                return param.test(value);
            }, "Invalid format.");
            $('#Registerform').validate({
                highlight: function (label) {
                    $(label).closest('.form-group').addClass('has-error');
                },
                unhighlight: function (label) {
                    $(label).closest('.form-group').removeClass('has-error');
                }
            });
            function isNumber(evt) {
                evt = (evt) ? evt : window.event;
                var charCode = (evt.which) ? evt.which : evt.keyCode;
                if (charCode > 31 && (charCode < 48 || charCode > 57)) {
                    return false;
                }
                return true;
            }
        });
    </script>
}




