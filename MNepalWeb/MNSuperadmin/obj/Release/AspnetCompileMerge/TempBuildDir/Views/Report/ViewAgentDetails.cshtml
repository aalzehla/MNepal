@model MNSuperadmin.Models.AgentProfileInfo
@{
    ViewBag.Title = "Registered Agent Report";
}

<!-- Content Header (Page header) -->
<section class="content-header">
    <h1>
        Report
        <small>View Registered Agents</small>
    </h1>
    <ol class="breadcrumb">
        <li><a href="#"><i class="fa fa-dashboard"></i> Report</a></li>
        <li class="active">Registered Agents</li>
    </ol>
</section>

<!-- Main content -->
<section class="content">
    <!-- Main row -->
    <div class="row">

        <div class="col-md-3">

            <!-- Profile Image -->
            <div class="box box-primary">
                <div class="box-body box-profile">
                    <img class="profile-user-img img-responsive img-circle" src="~/Images/default_icon.png" alt="User profile picture">
                    <h3 class="profile-username text-center">@Model.AgentName</h3>
                    <p class="text-muted text-center">Agent</p>
                </div>
                <!-- /.box-body -->
            </div>
            <!-- /.box -->

        </div>

        <!-- Start Form -->
        <div class="col-md-9">
            <div class="box box-primary">
                <div class="box-header with-border">
                    <h3 class="box-title">View Registration</h3>
                </div>
                <!-- /.box-header -->

                @using (Html.BeginForm())
                {
                    <div class="box-body">

                        <div class="form-horizontal">
                            <div class="form-group" style="display:none;">
                                @Html.LabelFor(model => model.ClientCode, htmlAttributes: new { @class = "control-label col-md-2" })
                                <div class="col-md-10">
                                    @ViewBag.ADClientCode
                                    @Html.EditorFor(model => model.ClientCode, new
                               {
                                   htmlAttributes = new
                                   {
                                       @class = "form-control",
                                       @readonly = "readonly"
                                   }
                               })
                                </div>
                            </div>
                        </div>

                        <strong><i class="fa fa-user margin-r-5"></i> User Information</strong>
                        <p></p>
                        <p class="text-muted"><b>Name : &nbsp; </b>  @Model.AgentName</p>
                        @*<p class="text-muted"><b>User Name : &nbsp; </b>  @Model.AgentName</p>*@
                        
                        <p class="text-muted"><b>Address : &nbsp; </b> @Model.Location</p>
                        
                        <hr />

                        <strong><i class="fa fa-user margin-r-5"></i> Status Information</strong>
                        <p></p>
                        <p class="text-muted"><b>Status : &nbsp; </b>  @Model.Status</p>
                        <p class="text-muted"><b>IsApproved : &nbsp; </b>  @Model.IsApproved</p>
                        <hr />

                        <strong><i class="fa fa-user margin-r-5"></i> Contact Information</strong>
                        <p></p>
                        <p class="text-muted"><b>Contact Number : &nbsp; </b>  @Model.AgentMobileNo</p>
                        @*<p class="text-muted" style="display:none;"><b>ContactNumber2 : &nbsp; </b>  @ViewBag.ContactNumber2</p>*@
                        <hr />

                        <strong><i class="fa fa-user margin-r-5"></i> Account Information</strong>
                        <p></p>
                        <p class="text-muted"><b>Wallet Number : &nbsp; </b>  @Model.WalletNumber</p>
                        <p class="text-muted"><b>Balance : &nbsp; </b>Rs. @Model.Balance</p>

                    </div>
                    <!-- /. box-body-->

                    <div class="box-footer">

                    </div>
                }

            </div>
        </div>
        <!--End Form -->

    </div>
    <!-- /.row -->
</section>
<!-- /.content -->
