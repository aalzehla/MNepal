
@{
    ViewBag.Title = "Modify Wallet Limit";
}

@model List<MNSuperadmin.Models.UserProfilesInfo>
<!-- =============================================== -->
<!-- Content Header (Page header) -->
<section class="content-header">
    <h1>

        Wallet Limit Setup
        @*<small>Modify Staff/Admin Role</small>*@
    </h1>
    <ol class="breadcrumb">
        <li><a href="#"><i class="fa fa-user"></i> Wallet limit</a></li>
        @*<li class="active"> Modify SuperAdmin Role</li>*@
    </ol>
</section>

<!-- Main content -->
<section class="content">
    <!-- Main row -->
    <div class="row">

        <!-- Start Form -->
        <div class="col-md-12">
            <div class="box box-primary">
                <div class="box-header with-border">
                    <h3 class="box-title">Wallet Limit</h3>
                </div>

                <!--Message-->
                @if (this.ViewData["admin_messsage"] != null)
                {
                    if (this.ViewData["message_class"].ToString() == "success_info")
                    {
                        <div class="alert alert-success">
                            <button type="button" class="close" data-dismiss="alert">&times;</button>
                            <h4>Success</h4> @this.ViewData["admin_messsage"]
                        </div>
                    }
                    else
                    {
                        <div class="alert alert-danger">
                            <button type="button" class="close" data-dismiss="alert">&times;</button>
                            <h4>Error</h4> @this.ViewData["admin_messsage"]
                        </div>
                    }
                }
                <!-- /.box-header -->
                <div class="box-body">

                    <div class="row">
                        <div class="col-md-10">

                            <table id="t01" class="table table-bordered table-striped">
                                <thead>
                                    <tr>
                                        <th>WalletProfileCode</th>
                                        @*<th>WFeatureCode</th>*@
                                        <th>WTxnCount</th>
                                        <th>WPerTxnAmt</th>
                                        <th>WPerDayAmt</th>
                                        <th>WTxnAmtM</th>
                                        @*<th>WTxnCode</th>*@
                                        <th colspan="3">Action</th>
                                    </tr>
                                </thead>
                                @foreach (var item in Model)
                                {
                                    <tr>
                                        <td>
                                            @item.WalletProfileCode
                                        </td>
                                        @*<td>
                                            @item.WFeatureCode
                                        </td>*@
                                        <td>
                                            @item.WTxnCount
                                        </td>
                                        <td>
                                            @item.WPerTxnAmt
                                        </td>
                                        <td>
                                            @item.WPerDayAmt
                                        </td>
                                        <td>
                                            @item.WTxnAmtM
                                        </td>
                                        @*<td>
                                            @item.WTxnCode
                                        </td>*@
                                        <td>
                                            
                                                <a href="@Url.Action("EditLimitSetup", "LimitSetup", new { walletProfileCode = @item.WalletProfileCode })"><span class="fa fa-edit" title="Edit"></span></a> @*Edit*@
                                            
                                        </td>
                                        @*<td style="width: 10px;">
                                            <a href="@Url.Action("ViewAdminProfileSetup", "SuperAdminRoleSetup", new {profileCode = @item.UPID})"><i class="fa fa-eye" title="View"></i></a>
                                        </td>*@
                                    </tr>

                                }

                            </table>

                        </div>
                    </div>

                </div>
            </div>
        </div>
        <!--End Form -->

    </div>
    <!-- /.row -->
</section>
<!-- /.content -->
<!-- page script -->
@section scripts{
    <script>
        $(function () {
            $('#t01').DataTable();
        });
    </script>
}
<!-- =============================================== -->
