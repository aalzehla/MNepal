@using MNSuperadmin.Models;
@{
    var tempDataProduct = TempData["FeaturedUserInfo"] as UserInfo;

    Layout = "~/Views/Shared/_Dashboard.cshtml";
    ViewBag.Title = "Dashboard";
    
}

<!-- Content Header (Page header) --> 
    <section class="content-header">
        <h1>
            Dashboard
            <small>Version 1.0.0</small>
            @*<small align="center">Super Admin without COC</small>*@
        </h1>
        <ol class="breadcrumb">
            <li><a href="#"><i class="fa fa-dashboard"></i> Home</a></li>
            <li class="active">Dashboard</li>
          
        </ol>
    </section>
    <h4 align="center">Super Admin without COC</h4>
    <!-- Main content --> 
    <section class="content">
        <!-- Main row -->
        <!-- Start Form -->
        <!-- Info boxes -->
        <div class="row">
            <div class="col-md-3 col-sm-6 col-xs-12">
                <div class="info-box">
                    <span class="info-box-icon bg-aqua"><i class="ion ion-ios-gear-outline"></i></span>
                    <div class="info-box-content">
                        <span class="info-box-text">Total Super Admins </span>
                        @*<span class="info-box-number">90<small>%</small></span>*@
                        <span class="info-box-number">@((ViewBag.TotalSuperAdmins as System.Data.DataTable).Rows.Count > 0 ? (ViewBag.TotalSuperAdmins as System.Data.DataTable).Rows[0]["TotalSuperAdmins"].ToString() : "0")</span>
                    </div>
                    <!-- /.info-box-content -->
                </div>
                <!-- /.info-box -->
            </div>
            <!-- /.col -->

            <div class="col-md-3 col-sm-6 col-xs-12">
                <div class="info-box">
                    <span class="info-box-icon bg-red"><i class="fa fa-user-plus"></i></span>
                    <div class="info-box-content">
                        <span class="info-box-text">Total Agents</span>
                        <span class="info-box-number">@((ViewBag.TotalAgents as System.Data.DataTable).Rows.Count > 0 ? (ViewBag.TotalAgents as System.Data.DataTable).Rows[0]["TotalAgents"].ToString() : "0")</span>
                    </div>
                    <!-- /.info-box-content -->
                </div>
                <!-- /.info-box -->
            </div>
            <!-- /.col -->
            <!-- fix for small devices only -->
            <div class="clearfix visible-sm-block"></div>

            <div class="col-md-3 col-sm-6 col-xs-12">
                <div class="info-box">
                    <span class="info-box-icon bg-green"><i class="ion ion-ios-cart-outline"></i></span>

                    <div class="info-box-content">
                        <span class="info-box-text">Total Customers</span>
                        <span class="info-box-number">@((ViewBag.TotalCustomers as System.Data.DataTable).Rows.Count > 0 ? (ViewBag.TotalCustomers as System.Data.DataTable).Rows[0]["TotalCustomers"].ToString() : "0")</span>
                    </div>
                    <!-- /.info-box-content -->
                </div>
                <!-- /.info-box -->
            </div>
            <!-- /.col -->

            <div class="col-md-3 col-sm-6 col-xs-12">
                <div class="info-box">
                    <span class="info-box-icon bg-yellow"><i class="ion ion-ios-people-outline"></i></span>

                    <div class="info-box-content">
                        <span class="info-box-text">Total Inactive Customers</span>
                        <span class="info-box-number">@((ViewBag.InActiveCustomers as System.Data.DataTable).Rows.Count > 0 ? (ViewBag.InActiveCustomers as System.Data.DataTable).Rows[0]["InActiveCustomers"].ToString() : "0")</span>

                        @*<span class="info-box-number">@((ViewBag.TotalNewMembers as System.Data.DataTable).Rows.Count > 0 ? (ViewBag.TotalNewMembers as System.Data.DataTable).Rows[0]["TotalNewMembers"].ToString() : "0")</span>*@
                    </div>
                    <!-- /.info-box-content -->
                </div>
                <!-- /.info-box -->
            </div>
            <!-- /.col -->

        </div>
        <!-- /.row-->

        <div class="row">
            <!-- Left col -->
            <div class="col-md-8">
                <!-- MAP & BOX PANE -->
                <div class="box box-success">
                    <div class="box-header with-border">
                        <h3 class="box-title">List of Expiring Customers(within 4 weeks)</h3>
                        <div class="box-tools pull-right">
                        </div>
                    </div>
                    <!-- /.box-header -->
                    <div class="box-body no-padding">
                        <div class="row">
                            <div class="col-md-12 col-sm-8">
                                <div class="pad">

                                    <table class="table table-condensed" id="users">
                                        <thead style="text-align:center">
                                            <tr>
                                                <th>Name</th>
                                                <th>Contact Number</th>
                                                <th>Expiry Date</th>
                                                <th>Expiry period in days</th>
                                            </tr>
                                        </thead>
                                        <tbody style="text-align:center">
                                            @if ((ViewBag.ExpiredCustomer as System.Data.DataTable).Rows.Count > 0)
                                            {

                                                foreach (System.Data.DataRow dr in (ViewBag.ExpiredCustomer as System.Data.DataTable).Rows)
                                                {
                                                    <tr>
                                                        <td align="left"> @dr["Name"] </td>
                                                        <td align="left"> @dr["ContactNumber"] </td>
                                                        <td align="left"> @dr["ExpiryDate"] </td>
                                                        <td> @dr["ExpiryPeriodInDays"] </td>
                                                    </tr>
                                                }
                                            }
                                        </tbody>
                                    </table>
                                    <a href="@Url.Action("CustomerExpiryList","Report")" class="elements"><span>More Customer Expiry List</span></a>
                                    <!-- for each end-->
                                </div>



                            </div>
                            <!-- /.col -->
                            <!-- /.col -->
                        </div>

                        <!-- /.row -->
                    </div>
                    <!-- /.box-body -->
                </div>
                <!-- /.box -->

                <div class="box box-success">
                    <div class="box-header with-border">
                        <h3 class="box-title">List of Admins</h3>
                        <div class="box-tools pull-right">
                        </div>
                    </div>
                    <div class="box-body no-padding">

                        <div class="row">
                            <div class="col-md-12 col-sm-8">
                                <div class="pad">

                                    <table class="table table-condensed" id="admins">
                                        <thead style="text-align:center">
                                            <tr>
                                                <th>Name</th>

                                            </tr>
                                        </thead>
                                        <tbody style="text-align:center">
                                            @if ((ViewBag.AdminList as System.Data.DataTable).Rows.Count > 0)
                                            {

                                                foreach (System.Data.DataRow dr in (ViewBag.AdminList as System.Data.DataTable).Rows)
                                                {
                                                    <tr>
                                                        <td align="left"> @dr["AdminList"] </td>

                                                    </tr>
                                                }
                                            }
                                        </tbody>
                                    </table>
                                    @*<a href="@Url.Action("CustomerExpiryList","Report")" class="elements"><span>More Customer Expiry List</span></a>*@
                                    <!-- for each end-->
                                </div>



                            </div>
                            <!-- /.col -->
                            <!-- /.col -->
                        </div>

                    </div>
                </div>


            </div>
            <!-- Left col -->
            <!-- Right col-->

            <div class="col-md-4">
                <div class="box box-primary">
                    <div class="box-header with-border">
                        <h3 class="box-title">Pending Requests(Branch Centric) </h3>
                    </div>
                    <!-- /.box-header -->
                    <div class="box-body">
                        <table class="table table-condensed">
                            <tbody>
                                <tr>
                                    <td>Super Admin Registration Approval(count)</td>
                                    <td align="right">@((ViewBag.SuperAdminRegistrationApproval as System.Data.DataTable).Rows.Count > 0 ? (ViewBag.SuperAdminRegistrationApproval as System.Data.DataTable).Rows[0]["SuperAdminRegistrationApproval"].ToString() : "0")</td>
                                </tr>
                                <tr>
                                    <td>Super Admin Modification Approval(count)</td>
                                    <td align="right">@((ViewBag.SuperAdminModificationApproval as System.Data.DataTable).Rows.Count > 0 ? (ViewBag.SuperAdminModificationApproval as System.Data.DataTable).Rows[0]["SuperAdminModificationApproval"].ToString() : "0")</td>
                                </tr>

                                <tr>
                                    <td>Agent Registration Approval(count)</td>
                                    <td align="right">@((ViewBag.AgentRegistrationApproval as System.Data.DataTable).Rows.Count > 0 ? (ViewBag.AgentRegistrationApproval as System.Data.DataTable).Rows[0]["AgentRegistrationApproval"].ToString() : "0")</td>
                                </tr>
                                <tr>
                                    <td>Agent Modification Approval(count)</td>
                                    <td align="right">@((ViewBag.AgentModificationApproval as System.Data.DataTable).Rows.Count > 0 ? (ViewBag.AgentModificationApproval as System.Data.DataTable).Rows[0]["AgentModificationApproval"].ToString() : "0")</td>
                                </tr>
                                <tr>
                                    <td> Customer KYC Verification(count)</td>
                                    <td align="right">@((ViewBag.CustomerWalletKYCVerification as System.Data.DataTable).Rows.Count > 0 ? (ViewBag.CustomerWalletKYCVerification as System.Data.DataTable).Rows[0]["CustomerWalletKYCVerification"].ToString() : "0")</td>
                                </tr>

                            </tbody>
                        </table>



                        <!-- /.item -->

                    </div>
                    <!-- /.box-body -->
                    <!-- /.box-footer -->
                </div>
                <!-- /.box -->

            </div>

            <!-- Right Col-->
        </div>

        <!-- /.row-->
        <!-- End Form -->
        @*<div class="row">

                @if (ViewBag.UserType == "admin")
                {
                    <h1>admin</h1>
                }
                else if (ViewBag.UserType == "agent")
                {
                    <h1>agent</h1>
                }
                else if (ViewBag.UserType == "user")
                {
                    <h1>User</h1>
                }



            </div>*@
        <!-- /.row -->
    </section>
    <!-- /.content -->
    @section scripts{
        <script type="text/javascript">
            localStorage.removeItem('activeTab');

            var registeredusers = $('#users').DataTable({
                "ordering": false,
                "filter": false,
                "sorting": false,
                "info": false,
                "lengthChange": false,
                "language": {
                    "emptyTable": "No records found matching the search parameter"
                }
            });

            var adminlist = $('#admins').DataTable({
                "ordering": false,
                "filter": false,
                "sorting": false,
                "info": false,
                "lengthChange": false,
                "language": {
                    "emptyTable": "No records found matching the search parameter"
                }
            });


        </script>
    }
