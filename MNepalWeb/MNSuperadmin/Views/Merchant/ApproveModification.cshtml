@model List<MNSuperadmin.Models.UserInfo>

@{
    ViewBag.Title = "SuperAdmin Registration rejected";
}

<!-- =============================================== -->
<!-- Content Header (Page header) -->
<section class="content-header">
    <h1>
        Merchant
        @*<small>Registration rejected</small>*@
    </h1>
    <ol class="breadcrumb">
        <li><a href="#"><i class="fa fa-user"></i> SuperAdmin</a></li>
        <li class="active">Merchant Registration Approved</li>
    </ol>
</section>

<!-- Main content -->
<section class="content">
    <!-- Main row -->
    <div class="row">

        <!-- Start -->

        <div class="col-xs-12">

            <!-- START UNAPPROVED ADMIN -->
            <div class="box box-primary">
                <div class="box-header with-border">
                    <h3 class="box-title">Merchant Registration Approved</h3>
                </div>

                @if (this.TempData["merchant_messsage"] != null)
                {
                    if (this.TempData["message_class"].ToString() == "success_info")
                    {
                        <div class="alert alert-success">
                            <button type="button" class="close" data-dismiss="alert">&times;</button>
                            <h4>Success</h4> @this.TempData["merchant_messsage"]
                        </div>
                    }
                    else
                    {
                        <div class="alert alert-danger">
                            <button type="button" class="close" data-dismiss="alert">&times;</button>
                            <h4>Error</h4> @this.TempData["merchant_messsage"]
                        </div>
                    }
                }
                <!-- /.box-header -->
                <div class="box-body">
                    @using (Html.BeginForm("ApproveModification", "Merchant", FormMethod.Get))
                    {
                        <div class="box-body">
                            <div class="row">
                                <!-- radio -->
                                <div class="form-group">
                                    <div class="radio col-md-12">
                                        <div class="col-md-12">
                                            <label>
                                                <input type="radio" name="Key" id="optionsRadios1" value="UserName" checked />
                                                <b>Merchant Name</b>
                                            </label>

                                        </div>
                                    </div>
                                </div>

                                <div class="form-group" id="form">
                                    <div class="col-md-6 form-group">
                                        <input type="text" class="form-control" id="txtUserName" name="Value" placeholder="Search Value" value="@ViewBag.Value" maxlength="25">
                                    </div>
                                    <div class="col-md-6 form-group">
                                        <input type="submit" class="btn btn-primary" id="btnSearch" value="Search" />
                                    </div>
                                </div>
                            </div>
                        </div>
                        <!-- /.box-body -->
                        @*<div class="box-footer">*@
                        @*<input type="submit" class="btn btn-primary" id="btnSearch" value="Search" />*@
                        @*</div>*@
                    }
                </div>

                <div class="box-body">

                    <table id="t01" class="table table-bordered table-striped">
                        <thead>
                            <tr>
                                <th>Merchant Name</th>
                                <th>User Name</th>
                                <th style="display:none;">ClientCode</th>
                                @*<th>Branch Code</th>
                        <th>Branch Name</th>*@
                                @*<th>Registration Number</th>*@
                                <th>Email Address</th>
                                <th colspan="3">Action</th>
                            </tr>
                        </thead>
                        @foreach (var item in Model)
                        {
                            <tr>
                                <td>
                                    @item.Name
                                </td>

                                <td>
                                    @item.UserName
                                </td>
                                <td style="display:none;">
                                    @item.ClientCode
                                </td>
                                @*<td>
                            @item.UserBranchCode
                        </td>
                        <td>
                            @item.UserBranchName
                        </td>*@
                                @*<td>
                          @item.RegistrationNumber
                        </td>*@
                                <td>
                                    @item.EmailAddress
                                </td>
                                <td>

                                    <a href="@Url.Action("ApproveRejectModify","Merchant", new {id = @item.ClientCode } )"><span class="fa fa-eye" title="View"></span></a> @*Edit*@

                                </td>

                            </tr>

                        }

                    </table>

                </div>

                <!-- /.box-body -->
            </div>
            <!-- /.box -->
            <!-- END UNAPPROVED ADMIN-->
            <!-- /.box -->
            <!-- END APPROVED ADMIN -->

        </div>
        <!-- End-->

    </div>
    <!-- /.row -->
</section>
<!-- /.content -->
<!-- =============================================== -->
