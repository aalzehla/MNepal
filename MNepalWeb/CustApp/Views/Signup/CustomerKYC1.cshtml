    <script type="text/javascript">

        var yearRangeDOB = "1944:+0";
        var yearRangeExpire = "1944:2033";
        $('#validateBSDate').hide();
        $('#validateADDate').hide();
        $('#firstName').hide();
        $('#lastName').hide();
        $('#country').hide();

        $('#DOB1').keypress(function (e) {
            e.preventDefault();
        });
        $('#BSDateOfBirth').keypress(function (e) {
            e.preventDefault();
        });

        function isNumber(evt) {
            evt = (evt) ? evt : window.event;
            var charCode = (evt.which) ? evt.which : evt.keyCode;
            if ((charCode > 32 && charCode < 48) || (charCode > 57 && charCode < 65) || (charCode > 90 && charCode < 97) || (charCode > 122 && charCode < 127) || (charCode > 47 && charCode < 58) ){
                checkValidDate();
                return false;
            }
            checkValidDate();
            return true;
        }

        function isAlphabateDot(evt) {
            evt = (evt) ? evt : window.event;
            var charCode = (evt.which) ? evt.which : evt.keyCode;
            if ((charCode > 32 && charCode < 46) || (charCode > 46 && charCode < 48) || (charCode > 57 && charCode < 65) || (charCode > 90 && charCode < 97) || (charCode > 122 && charCode < 127) || (charCode > 47 && charCode < 58)) {
                checkValidDate();
                return false;
            }
            checkValidDate();
            return true;
        }

        function checkValidDate() {
            var fName = $('#FName').val();
            var LName = $('#LastName').val();
            var ADDate = $('#DOB').val();
            var BSDate = $('#BSDateOfBirth').val();
            var countryName = $('#Country').val();
            var result1 = false, result2 = false, result3 = false;
            debugger;
            if (fName.trim() == "") {
                $("#firstName").html("");
                $("#firstName").append("<small>" + "The First Name is required and cannot be empty" + "</small>");
                $("#firstName").show();
                $('#btnSubmit').attr('disabled', 'disabled');
                result1= false;
            }
            else {
                $("#firstName").hide();
                result1 = true;
            }
            if (LName.trim() == "") {
                $("#lastName").html("");
                $("#lastName").append("<small>" + "The Last Name is required and cannot be empty" + "</small>");
                $("#lastName").show();
                $('#btnSubmit').attr('disabled', 'disabled');
               
            }
            else {
                $("#lastName").hide();
                result2 = true;
            }

            if (ADDate == "") {
                $("#validateADDate").html("");
                $("#validateADDate").append("<small>" + "The DOB is required and cannot be empty" + "</small>");
                $('#validateADDate').show();
                $("#validateBSDate").html("");
                $("#validateBSDate").append("<small>" + "The DOB is required and cannot be empty" + "</small>");
                $('#validateBSDate').show();
                $('#btnSubmit').attr('disabled', 'disabled');
              
            }
            else {
                $('#validateADDate').hide();
                $('#validateBSDate').hide();
               
                result3 = true;
            }
            if (result1 && result2 && result3) {
                $('#btnSubmit').removeAttr('disabled');
            }
            return result1;
