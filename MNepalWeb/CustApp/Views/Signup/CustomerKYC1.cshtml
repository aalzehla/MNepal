        }

        //start milayako
        $(document).ready(function () {
            $('#BSDiv').hide();

            $("#DOB1").datepicker({
                dateFormat: 'yy-mm-dd',
                //yearRange: "-100:+0",
                yearRange: yearRangeDOB,
                changeMonth: true,
                changeYear: true,
                maxDate: new Date
            });

            $('#DOB1').change(function () {


                $('#BSDateOfBirth').val(AD2BS($('#DOB1').val()));
                var ADdate = $('#DOB1').val();
                $('#DOB').val(ADdate);
                var test = $('#DOB').val();
                var date = new Date(ADdate);
