                                        </div>

                                    </div>


                                    <div class="form-group">
                                        <label class="control-label">Occupation<b style="color:red;">*</b></label>



                                        <select class="form-control select2" id="Occupation" name="txtOccupation" style="width: 100%;">
                                            @*<option value="SelectOccupation" selected="selected" disabled>----Select Occupation----</option>*@
                                            <option value="Govermental Service" selected="selected">Govermental Service</option>
                                            <option value="Private Service">Private Service</option>
                                            <option value="Self Employed">Self Employed</option>

                                            <option value="Professional">Professional</option>
                                            <option value="Business">Business</option>
                                            <option value="PublicSector">Public Service</option>
                                            <option value="Others">Others</option>

                                        </select>
                                    </div>

                                    <div class="form-group">
                                        <label class="control-label">Email Address<b style="color:red;">*</b></label>
                                        <input type="text" class="form-control" id="Email" name="txtEmail" value="@Model.EmailAddress" data-target="tdEmail" placeholder="email@email.com" style="margin-bottom:3px;" />

                                    </div>

                                    <div class="form-group" hidden>
                                        <label class="control-label"> Pan No. </label>
                                        <input type="text" class="form-control" id="PanNo" name="txtPanNo" placeholder="Pan Number" maxlength="10" onkeypress="return isNumberKey(event)" />

                                    </div>



                                    <div class="btn-wrapper">
                                        @*<button class="btn btn-blue">Continue</button>*@
                                        <input type="submit" class="btn btn-blue " id="btnSubmit" name="txtSubmit" value="Continue" />

                                    </div>


                                }


                            </div>
