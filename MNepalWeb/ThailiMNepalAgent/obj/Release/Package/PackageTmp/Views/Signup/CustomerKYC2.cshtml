                                        <div class="form-group">
                                            <label class="control-label">Spouse Name<b style="color:red;">*</b></label>
                                            @Html.TextBoxFor(x => x.SpouseName, new { @class = "form-control", @placeholder = "Spouse Name", @onkeypress = "return isAlphabateDot(event)", @style ="margin-bottom:3px;" })


                                        </div>


                                        <div class="form-group">
                                            <label class="control-label">Father In law's Name<b style="color:red;">*</b></label>
