        function checkValidDate() {
            var fName = $('#FName').val();
            var LName = $('#LastName').val();
            var ADDate = $('#DOB').val();
            var BSDate = $('#BSDateOfBirth').val();
            var countryName = $('#Country').val();

            if (fName.trim() == "") {
                $("#firstName").html("");
                $("#firstName").append("<small>" + "The First Name is required and cannot be empty" + "</small>");
                $("#firstName").show();
                $('#btnSubmit').attr('disabled', 'disabled');
            }
            else {
                $("#firstName").hide();
                $('#btnSubmit').removeAttr('disabled');
            }
            if (LName.trim() == "") {
                $("#lastName").html("");
                $("#lastName").append("<small>" + "The Last Name is required and cannot be empty" + "</small>");
                $("#lastName").show();
                $('#btnSubmit').attr('disabled', 'disabled');
            }
            else {
                $("#lastName").hide();
                $('#btnSubmit').removeAttr('disabled');
            }

            if (ADDate == "") {
                $("#validateADDate").html("");
                $("#validateADDate").append("<small>" + "The DOB is required and cannot be empty" + "</small>");
                $('#validateADDate').show();
                $("#validateBSDate").html("");
                $("#validateBSDate").append("<small>" + "The DOB is required and cannot be empty" + "</small>");
                $('#validateBSDate').show();
                $('#btnSubmit').attr('disabled', 'disabled');
            }
            else {
                $('#validateADDate').hide();
                $('#validateBSDate').hide();
                $('#btnSubmit').removeAttr('disabled');
            }

            if (countryName.trim() == "") {
                $("#country").html("");
                $("#country").append("<small>" + "The Country is required and cannot be empty" + "</small>");
                $("#country").show();
                $('#btnSubmit').attr('disabled', 'disabled');
            }
            else {
                $("#country").hide();
                $('#btnSubmit').removeAttr('disabled');
            }
        }

