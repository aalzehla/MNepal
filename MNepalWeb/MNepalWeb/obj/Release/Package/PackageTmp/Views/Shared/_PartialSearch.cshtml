


<div class="box-body">
    <div class="row">
        <!-- radio -->
        <div class="form-group">
            <div class="radio col-md-12">
                <div class="col-md-12">
                    <label>
                        <input type="radio" name="optionsRadios" id="optionsRadios1" value="Mobile Number" />
                        <b>Mobile Number</b>
                    </label>
                    &nbsp;
                    <label>
                        <input type="radio" name="optionsRadios" id="optionsRadios2" value="Name" />
                        <b>Name</b>
                    </label>
                    &nbsp;
                    <label>
                        <input type="radio" name="optionsRadios" id="optionsRadios3" value="Account Number">
                        <b>AccountNumber</b>
                    </label>
                    &nbsp;
                    <label>
                        <input type="radio" name="optionsRadios" id="optionsRadios4" value="Name/Account">
                        <b>Name/Account</b>
                    </label>
                    &nbsp;
                    <label>
                        <input type="radio" name="optionsRadios" id="optionsRadios5" value="All" checked>
                        <b>All</b>
                    </label>
