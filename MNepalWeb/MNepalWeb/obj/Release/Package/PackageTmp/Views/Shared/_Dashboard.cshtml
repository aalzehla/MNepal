    <script>
        $(document).ready(function () {

            //var $table = $('.sticky-header');
            //$table.floatThead({
            //    scrollContainer: function ($table) {
            //        return $table.closest('div');
            //    }
            //});


            $('.treeview-menu li a').on('click', function (e) {
                var current = $(this);
                localStorage.setItem('activeTab', $('.treeview-menu li a').index(current));
            });

            //$('.removeactive').on('click', function () {
            //    if (activeTab)
            //        localStorage.removeItem('activeTab');
            //});

            var activeTab = localStorage.getItem('activeTab');
            if (activeTab) {
                activeTabEL = $('.treeview-menu li a').eq(parseInt(activeTab));
                activeTabEL.parent().addClass('active');
                activeTabEL.parent().parent().css('display', 'block');
                activeTabEL.parent().parent().parent().addClass('menu-open');
            }

        
            




            $.fn.dataTable.ext.errMode = 'none';

            $('#SearchResultTable').on('error.dt', function (e, settings, techNote, message) {
                $('#loading').hide();
                $('#btnSearch').prop("disabled", false);
                $('#SearchResultTable').hide();
                if ($.fn.DataTable.isDataTable('#SearchResultTable')) {
                    $('#SearchResultTable').DataTable().destroy();
                }
                var msg = message.split("-");
                if (msg.length > 1)
                    alertify.error('Error : ' + msg[msg.length - 1]);
                else
                    alertify.error('Error : ' + msg[msg.length - 1]);

            })






        });
    </script>





