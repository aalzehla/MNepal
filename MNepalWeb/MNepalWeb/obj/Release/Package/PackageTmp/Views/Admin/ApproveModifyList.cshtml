@model List<MNepalWeb.Models.UserInfo>

@{
    ViewBag.Title = "ApproveModifyList";
}

<!-- Content Header (Page header) -->
<section class="content-header">
    <h1>
        Admin
        <small>Approve Modification List</small>
    </h1>
    <ol class="breadcrumb">
        <li><a href="#"><i class="fa fa-dashboard"></i> Dashboard</a></li>
        <li class="active"> ApproveModify List</li>
    </ol>
</section>

<!-- Main content -->
<section class="content">
    <!-- Main row -->
    <div class="row">

        <!-- Start Form -->
        <div class="col-md-12">
            <div class="box box-primary">
                <div class="box-header with-border">
                    <h3 class="box-title">Admin Approve Details</h3>
                </div>
                <!-- /.box-header -->
                <div class="box-body">

                    @if (ViewData["dtableApproveList"] == null)
                    {
                        <div class="panel-body"> <b>No Result Found !! </b> </div>
                    }
                    else if (ViewData["dtableApproveList"] != null)
                    {

                        <table id="t01" class="table table-bordered table-striped">
                        <thead>
                            <tr>
                                <th>Name</th>
                                <th>Mobile Number</th>
                                <th>UserName</th>
                                <th>UserType</th>
                                <th>Status</th>
                                <th colspan="3">Action</th>
                            </tr>
                        </thead>
                        @foreach (var item in Model)
                        {
                            <tr>
                                <td>
                                    @item.ClientCode - @item.Name
                                </td>
                                <td>
                                    @item.ContactNumber1
                                </td>
                                <td>
                                    @item.UserName
                                </td>
                                <td>
                                    @item.UserType
                                </td>
                                <td>
                                    @item.Status
                                </td>
                                <td>
                                    <a href="@Url.Action("ApproveModify", "Admin", new { clientCodeID = @item.ClientCode })"><i class="fa fa-edit"></i></a> @*Edit*@
                                </td>
                            </tr>

                        }

                    </table>

                    }

                </div>

            </div>
        </div>

        <!--End Form -->

    </div>
    <!-- /.row -->
</section>
<!-- /.content -->
<!-- page script -->
@section scripts{
    <script>
        $(function() {
            $('#t01').DataTable();
        });
    </script>
}
