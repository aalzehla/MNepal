
@{
    ViewBag.Title = "RejectedList";
}

<!-- =============================================== -->
<!-- Content Header (Page header) -->
<section class="content-header">
    <h1>
        Customer
        <small>Rejected List</small>
    </h1>
    <ol class="breadcrumb">
        <li><a href="#"><i class="fa fa-user"></i> Customer</a></li>
        <li class="active"> Rejected List</li>
    </ol>
</section>

<!-- Main content -->
<section class="content">
    <!-- Main row -->
    <div class="row">

        <!-- Start -->

        <div class="col-xs-12">

            <!-- START UNAPPROVED CUSTOMER -->
            <div class="box box-primary">
                <div class="box-header with-border">
                    <h3 class="box-title">Rejected List For UnApproved Customer</h3>
                </div>
                <!-- /.box-header -->
                <div class="box-body">
                    @if (ViewData["UNApproveReject"] == null)
                    {
                        <div class="panel-body"> <b>No Pending requests for unapproval !! </b> </div>
                    }
                    else if (ViewData["UNApproveReject"] != null)
                    {
                        <table id="t01" class="table table-bordered table-striped">
                            <thead>
                                <tr>
                                    <th>Name</th>
                                    <th>Address</th>
                                    <th>UserName</th>
                                    <th>ContactNumber1</th>
                                    <th>Status</th>
                                    <th>IsApproved</th>
                                    <th>IsRejected</th>
                                </tr>
                            </thead>
                            <tbody>

                                @foreach (System.Data.DataRow dr in (ViewData["UNApproveReject"] as System.Data.DataTable).Rows)
                                {
                                    <tr>
                                        <td> @dr["Name"] </td>
                                        <td> @dr["Address"] </td>
                                        <td> @dr["UserName"] </td>
                                        <td> @dr["ContactNumber1"] </td>
                                        <td> @dr["Status"] </td>
                                        <td> @dr["IsApproved"] </td>
                                        <td> <span class="badge bg-red"> @dr["IsRejected"] </span> </td>
                                    </tr>
                                } <!-- for each end-->

                            </tbody>
                            <tfoot>

                            </tfoot>
                        </table>
                    }
                </div>
                <!-- /.box-body -->
            </div>
            <!-- /.box -->
            <!-- END UNAPPROVED CUSTOMER -->
            <!-- START APPROVED CUSTOMER -->
            <div class="box box-success">
                <div class="box-header with-border">
                    <h3 class="box-title">Rejected List For Approved Customer</h3>
                </div>
                <!-- /.box-header -->
                <div class="box-body">
                    @if (ViewData["ApproveReject"] == null)
                    {
                        <div class="panel-body"> <b>No Pending requests for approval !! </b></div>
                    }
                    else if (ViewData["ApproveReject"] != null)
                    {

                        <table id="t02" class="table table-bordered table-striped">
                            <thead>
                                <tr>
                                    <th style="width: 20px">Name</th>
                                    <th>Address</th>
                                    <th>UserName</th>
                                    <th>ContactNumber1</th>
                                    <th>Status</th>
                                    <th>IsApproved</th>
                                    <th>IsRejected</th>
                                </tr>
                            </thead>
                            <tbody>

                                @foreach (System.Data.DataRow dr in (ViewData["ApproveReject"] as System.Data.DataTable).Rows)
                                {
                                    <tr>
                                        <td> @dr["Name"] </td>
                                        <td> @dr["Address"] </td>
                                        <td> @dr["UserName"] </td>
                                        <td> @dr["ContactNumber1"] </td>
                                        <td> @dr["Status"] </td>
                                        <td> @dr["IsApproved"] </td>
                                        <td>
                                            <span class="label label-danger"> @dr["IsRejected"] </span>
                                        </td>
                                    </tr>
                                } <!-- for each end-->

                            </tbody>
                            <tfoot>

                            </tfoot>
                        </table>
                    }
                </div>
                <!-- /.box-body -->
            </div>
            <!-- /.box -->
            <!-- END APPROVED CUSTOMER -->

        </div>
        <!-- End-->

    </div>
    <!-- /.row -->
</section>
<!-- /.content -->
<!-- =============================================== -->
