                                                                    item.OldValue = "Custom Date";
                                                                }
                                                                else if (item.OldValue == "Yr")
                                                                {
                                                                    item.OldValue = "Year";
                                                                }
                                                                else if (item.OldValue == "U")
                                                                {
                                                                    item.OldValue = "Unlimited";
                                                                }
                                                                else if (item.OldValue == "L")
                                                                {
                                                                    item.OldValue = "Limited";
