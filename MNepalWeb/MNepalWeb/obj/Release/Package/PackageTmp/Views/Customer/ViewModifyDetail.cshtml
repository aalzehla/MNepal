                                                                @if (item.NewValue == "En")
                                                                {
                                                                    item.NewValue = "Enable";
                                                                }
                                                                else if (item.NewValue == "Dis")
                                                                {
                                                                    item.NewValue = "Disable";
                                                                }
                                                                else if (item.NewValue == "Mo")
                                                                {
                                                                    item.NewValue = "Month";
                                                                }
                                                                else if (item.NewValue == "We")
