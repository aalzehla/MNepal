                                                            <tr>
                                                                @if (item.ColumnName == "IndvTxn")
                                                                {
                                                                    item.ColumnName = "Individual Transaction Limit";
                                                                }
                                                                <td>@item.ColumnName</td>
                                                                @if (item.OldValue == "En")
                                                                {
                                                                    item.OldValue = "Enable";
                                                                }
                                                                else if (item.OldValue == "Dis")
                                                                {
                                                                    item.OldValue = "Disable";
                                                                }
                                                                else if (item.OldValue == "Mo")
                                                                {
                                                                    item.OldValue = "Month";
                                                                }
                                                                else if (item.OldValue == "We")
                                                                {
                                                                    item.OldValue = "Week";
                                                                }
                                                                else if (item.OldValue == "Cu")
