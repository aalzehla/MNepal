

@model List<MNepalWeb.Models.UserInfo>

@{
    ViewBag.Title = "AdminDetails";
}

<!-- =============================================== -->
<!-- Content Header (Page header) -->
<section class="content-header">
    <h1>
        Report
        <small>AdminDetails</small>
    </h1>
    <ol class="breadcrumb">
        <li><a href="#"><i class="fa fa-user"></i> Report</a></li>
        <li class="active"> Admin Details</li>
    </ol>
</section>

<!-- Main content -->
<section class="content">
    <!-- Main row -->
    <div class="row">

        <!-- Start Form -->
        <div class="col-md-12">
            <div class="box box-primary">
                <div class="box-header with-border">
                    <h3 class="box-title">Admin Details</h3>
                </div>
                <!-- /.box-header -->

               

                <div class="box-body">

                    <table id="t01" class="table table-bordered table-striped">
                        <thead>
                            <tr>
                                <th>Admin Name</th>
                                @*<th>Mobile Number</th>*@
                                <th>User Name</th>
                                <th>Branch Name</th>
                                <th>Email Address</th>
                                <th>User Role</th>
                                @*<th>Created By</th>*@
                                <th>Status</th>
                                
                            </tr>
                        </thead>
                        @foreach (var item in Model)
                        {
                            <tr>
                                <td style="display:none;">
                                    @item.ClientCode 
                                </td>
                                <td>
                                      @item.Name
                                </td>
                                <td>
                                    @item.UserName
                                </td>
                                <td>
                                    @item.UserBranchName
                                </td>
                                <td>
                                    @item.EmailAddress
                                </td>
                                <td>
                                    @item.AProfileName
                                </td>
                                @*<td>
                                        @item.CreatedBy
                                    </td>*@
                                <td>
                                    @item.Status
                                </td>
                                
                               
                            </tr>

                        }

                    </table>

                </div>

            </div>
        </div>

        <!--End Form -->

    </div>
    <!-- /.row -->
</section>
<!-- /.content -->
<!-- page script -->
@section scripts{

}

<!-- =============================================== -->
