
@{
    ViewBag.Title = "ModifyAdmin";
}


@model List<MNepalWeb.Models.UserInfo>
<!-- =============================================== -->
<!-- Content Header (Page header) -->
<section class="content-header">
    <h1>
        Admin Modification
        <small>Modify</small>
    </h1>
    <ol class="breadcrumb">
        <li><a href="#"><i class="fa fa-dashboard"></i> Dashboard</a></li>
        <li class="active">ModifyAdmin</li>
    </ol>
</section>

<!-- Main content -->
<section class="content">
    <!-- Main row -->
    <div class="row">

        <!-- Start Form -->
        <div class="col-md-12">
            <div class="box box-primary">
                <div class="box-header with-border">
                    <h3 class="box-title">Manage Admin Profiles</h3>
                </div>
                <!-- /.box-header -->

                <div class="box-body">

                    <table id="t01" class="table table-bordered table-striped">
                        <thead>
                            <tr>
                                <th>Name</th>
                                <th>Address</th>
                                <th>UserName</th>
                                <th>Status</th>
                                <th colspan="3">Action</th>
                            </tr>
                        </thead>
                        @foreach (var item in Model)
                        {
                            <tr>
                                <td>
                                    @item.Name
                                </td>
                                <td>
                                    @item.Address
                                </td>
                                <td>
                                    @item.UserName
                                </td>
                                <td>
                                    @item.Status
                                </td>
                                <td>
                                    <a href="@Url.Action("EditRegistration","Admin", new {clientCodeID = @item.ClientCode } )"><i class="fa fa-edit"></i></a> @*Edit*@
                                </td>
                                <td>
                                    <a href="@Url.Action("ViewRegistration","Admin", new {clientCodeID = @item.ClientCode } )"><i class="fa fa-eye"></i></a>
                                    
                                </td>
                                <td>
                                    <a href="@Url.Action("ModifyStatusAdmin","ProfileSetup", new {clientCodeID = @item.ClientCode } )"><i class="fa fa-trash-o"></i></a>

                                </td>
                            </tr>

                        }

                    </table>

                </div>

            </div>
        </div>
        <!--End Form -->

    </div>
    <!-- /.row -->
</section>
<!-- /.content -->
<!-- =============================================== -->
@section scripts{
    <script>
        $(function() {
            $('#t01').DataTable();
        });
    </script>
}