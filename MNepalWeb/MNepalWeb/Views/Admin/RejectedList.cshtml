@model List<MNepalWeb.Models.UserInfo>

@{
    ViewBag.Title = "Admin Modification Rejected";
}

<!-- =============================================== -->
<!-- Content Header (Page header) -->
<section class="content-header">
    <h1>
       Admin
        @*<small></small>*@
    </h1>
    <ol class="breadcrumb">
        <li><a href="#"><i class="fa fa-user"></i> Admin</a></li>
        <li class="active">Modification Rejected</li>
    </ol>
</section>

<!-- Main content -->
<section class="content">
    <!-- Main row -->
    <div class="row">

        <!-- Start -->

        <div class="col-xs-12">

            <!-- START UNAPPROVED ADMIN -->
            <div class="box box-primary">
                <div class="box-header with-border">
                    <h3 class="box-title">Modification Rejected</h3>
                </div>

                @if (this.TempData["custmodify_messsage"] != null)
                {
                    if (this.TempData["message_class"].ToString() == "success_info")
                    {
                        <div class="alert alert-success">
                            <button type="button" class="close" data-dismiss="alert">&times;</button>
                            <h4>Success</h4> @this.TempData["custmodify_messsage"]
                        </div>
                    }
                    else
                    {
                        <div class="alert alert-danger">
                            <button type="button" class="close" data-dismiss="alert">&times;</button>
                            <h4>Error</h4> @this.TempData["custmodify_messsage"]
                        </div>
                    }
                }
                <!-- /.box-header -->
                <div class="box-body">

                    <table id="t01" class="table table-bordered table-striped">
                        <thead>
                            <tr>
                                <th>Name</th>
                                <th>User Name</th>
                                <th style="display:none;">ClientCode</th>
                                <th>Branch Code</th>
                                <th>Branch Name</th>
                                <th>Email Address</th>
                                <th colspan="3">Action</th>
                            </tr>
                        </thead>
                        @foreach (var item in Model)
                        {
                            <tr>
                                <td>
                                   @item.Name
                                </td>
                                
                                <td>
                                    @item.UserName
                                </td>
                                <td style="display:none;">
                                    @item.ClientCode
                                </td>
                                <td>
                                    @item.UserBranchCode
                                </td>
                                <td>
                                    @item.UserBranchName
                                </td>
                                <td>
                                    @item.EmailAddress
                                </td>
                               <td>
                                   <a href="@Url.Action("EditRegistration","Admin", new {clientCodeID = @item.ClientCode } )"><span class="fa fa-edit" title="Edit"></span></a> @*Edit*@
                                  @*<a href="@Url.Action("ViewAdminRejected","Admin", new {clientCodeID = @item.ClientCode } )"><span class="fa fa-edit" title="Edit"></span></a> @*Edit*@
                                </td>
                               
                            </tr>

                        }

                    </table>

                </div>
                 
                <!-- /.box-body -->
            </div>
            <!-- /.box -->
            <!-- END UNAPPROVED ADMIN-->
           
            <!-- /.box -->
            <!-- END APPROVED ADMIN -->

        </div>
        <!-- End-->

    </div>
    <!-- /.row -->
</section>
<!-- /.content -->
<!-- =============================================== -->
