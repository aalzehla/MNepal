
@model List<MNepalWeb.Models.UserInfo>

@{
    ViewBag.Title = "Admin Modify";
}

<!-- =============================================== -->

<!-- Content Header (Page header) -->
<section class="content-header">
    <h1>
        Admin
        @*<small>Modify</small>*@
    </h1>
    <ol class="breadcrumb">
        <li><a href="#"><i class="fa fa-user"></i> Admin</a></li>
        <li class="active"> Modify</li>
    </ol>
</section>

<!-- Main content -->
<section class="content">
    <!-- Main row -->
    <div class="row">

        <!-- Start Form -->
        <div class="col-md-12">
            <div class="box box-primary">
                <div class="box-header with-border">
                    <h3 class="box-title">Modify(Admin Registration Details)</h3>
                </div>
                <!-- /.box-header -->

                @if (this.TempData["editregister_messsage"] != null)
                {
                if (this.TempData["message_class"].ToString() == "success_info")
                {
                <div class="alert alert-success">
                    <button type="button" class="close" data-dismiss="alert">&times;</button>
                    <h4>Success</h4> @this.TempData["editregister_messsage"]
                </div>
                }
                else
                {
                <div class="alert alert-danger">
                    <button type="button" class="close" data-dismiss="alert">&times;</button>
                    <h4>Error</h4> @this.TempData["registration_message"]
                </div>
                }
                }

                <div class="box-body">

                    <table id="t01" class="table table-bordered table-striped">
                        <thead>
                            <tr>
                                <th>Admin Name</th>
                                @*<th>Mobile Number</th>*@
                                <th>User Name</th>
                                <th>Branch Code</th>
                                <th>Branch Name</th>
                                <th>Email Address</th>
                                <th>User Role</th>
                                @*<th>Created By</th>*@
                                <th>Status</th>                                                         
                                <th colspan="3">Action</th>
                            </tr>
                        </thead>
                        @foreach (var item in Model)
                        {
                            <tr>
                                <td style="display:none;">@item.ClientCode</td>
                                <td>
                                   @item.Name
                                </td>
                                @*<td>
                                    @item.ContactNumber1
                                </td>*@
                                <td>
                                    @item.UserName
                                </td>
                                <td>
                                    @item.UserBranchCode
                                </td>
                                <td>
                                    @item.UserBranchName
                                </td>
                                <td>
                                    @item.EmailAddress
                                </td>
                                <td>
                                    @item.AProfileName
                                </td>
                                @*<td>
                                    @item.CreatedBy
                                </td>*@
                                <td>
                                    @item.Status
                                </td>                                                       
                                <td>
                                    @if ((string)Session["LOGGED_USERNAME"] != item.UserName)

                                    {
                                        <a href="@Url.Action("EditRegistration","Admin", new {clientCodeID = @item.ClientCode } )"><span class="fa fa-edit" title="Edit"></span></a> @*Edit*@
                                    }
                                  
                                </td>
                                @*<td>
                                    <a href="@Url.Action("ViewRegistration","Admin", new {clientCodeID = @item.ClientCode } )"><i class="fa fa-eye"></i></a>
                                </td>*@
                            </tr>

                        }

                    </table>

                </div>

            </div>
        </div>

        <!--End Form -->

    </div>
    <!-- /.row -->
</section>
<!-- /.content -->
<!-- page script -->
@section scripts{
   
}

<!-- =============================================== -->
