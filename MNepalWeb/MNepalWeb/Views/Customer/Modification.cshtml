        //COPYING VALUES OF PERMANENT ADDRESS TO CURRENT ADDRESS---
        $(document).ready(function () {
            $("#copyvalues").on("click", function () {
                //e.preventDefault();
                var permanentAddress = $("#permanentAddress input[type='text']");
                var currentAddress = $("#currentAddress input[type='text']");
                var checkboxINstance = $(this);
                if (!checkboxINstance.is(":checked")) {
                    $("#currentAddress input[type='text']")[0].value = '';

                } else {
                    for (var i = 0; i < permanentAddress.length; i++) {
                        var permanentAddressValue = permanentAddress.eq(i).val();
                        var permanentAddresId = $("#permanentAddress input[type='text']")[i].id;
                        var currentAddressId = $("#currentAddress input[type='text']")[i].id;
                        $("#" + currentAddressId).val(permanentAddressValue);
                        //for readonly= true when checkbox is ticked
                        document.getElementById(currentAddressId).readOnly = true;
                        document.getElementById(permanentAddresId).readOnly = true;
                    }
                }
                if (!checkboxINstance.is(":checked")) {
                    $('#CStreet, #CHouseNo, #CMunicipalityVDC, #CWardNo').val('');
                    document.getElementById("CStreet").readOnly = false;
                    document.getElementById("CMunicipalityVDC").readOnly = false;
                    document.getElementById("CHouseNo").readOnly = false;
                    document.getElementById("CWardNo").readOnly = false;
                    document.getElementById("PStreet").readOnly = false;
                    document.getElementById("PMunicipalityVDC").readOnly = false;
                    document.getElementById("PProvince").disabled = false;
                    document.getElementById("PHouseNo").readOnly = false;
                    document.getElementById("PWardNo").readOnly = false;
                }
            });
            $('#copyvalues').on(':checked').click(function () {
                var checkboxINstance = $(this);
                var $options = $("#PProvince > option").clone();
                if (checkboxINstance.is(":checked")) {
                    $('#CProvince').empty();
                    $('#CProvince').append($options);
                    $('#CProvince').val($('#PProvince').val());
                    $("#CProvinceText").val($('#CProvince').val());
                    //$("#PProvinceText").val($('#PProvince').val());
                    document.getElementById("CProvince").disabled = true;
                    document.getElementById("PProvince").disabled = true;
                } else {
                    $('#CProvince').val('');
                    $('#CProvinceText').val('');
                    document.getElementById("CProvince").disabled = false;
                    document.getElementById("PProvince").disabled = false;
                }
            });
