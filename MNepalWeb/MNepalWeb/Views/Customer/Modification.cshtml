                    $("#image").html("Image size must not exceed more than 2 MB.");
                    $("#image").css("color", "#dd4b39");
                }
                else {
                    $("#image").html("Invalid Image format!");
                    $("#image").css("color", "#dd4b39");
                }
            });
        });

