@section scripts{
    <script>

        function isNumber(evt) {
            evt = (evt) ? evt : window.event;
            var charCode = (evt.which) ? evt.which : evt.keyCode;
            if (charCode > 31 && (charCode < 48 || charCode > 57)) {
                return false;
            }
            return true;
        }

        $(document).ready(function () {
            var x = $('#IdentificationType').val();
            DisplayDiv(x);

        });
        $('#addoccu').click(function (e) {
            e.preventDefault();
        });
        $('#IdentificationType').change(function () {
            var value = $(this).val();
            DisplayDiv(value);

        });

        $('input[type=radio][name=MaritalStatus]').change(function () {
            var val = $(this).val();
            if (val != "U") {
                $('#family').show(500);
            }
            else {
                $('#family').hide(500);
            }
        });

        function DisplayDiv(value) {
            if (value == "C") {
                $('#Citizenship').show(500);
                $('#Passport').hide(500);
            }
            if (value == "P") {
                $('#Passport').show(500);
                $('#Citizenship').hide(500);
            }

        }


        function showImage(fileInput) {
            var files = fileInput.files;
            var file = files[0];
            var imageType =  /image.*/;
            var srcid = fileInput.id;
            debugger;
            if (!file.type.match(imageType)) {
                //$("#file").val = "";
                document.getElementById(srcid).value = "";
                debugger;
                var error = fileInput.dataset.error;
                document.getElementById(error).innerHTML = "Can only upload JPEG images.";

            }
            else {
                var reader1 = new FileReader();
                reader1.onload = (function (aImg) {
                    return function (e) {
                        aImg.src = e.target.result;
                    };
                });
                reader1.readAsDataURL(file);
                var destination = fileInput.dataset.target;
                var img = document.getElementById(destination);
                img.file = file;
                var reader = new FileReader();
                reader.onload = (function (aImg) {
                    return function (e) {
                        aImg.src = e.target.result;
                    };
                })(img);
                reader.readAsDataURL(file);
            }
        }
    </script>
}
