@model IEnumerable<MNepalProject.Models.Feature>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.FeeMaster.ID)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.LimitMaster.ID)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ProductMaster.ProductName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.FeatureCode)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.SourceBIN)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.DestinationBIN)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.FeeMaster.ID)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.LimitMaster.ID)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ProductMaster.ProductName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.FeatureCode)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.SourceBIN)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.DestinationBIN)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.ID }) |
            @Html.ActionLink("Details", "Details", new { id=item.ID }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.ID })
        </td>
    </tr>
}

</table>
